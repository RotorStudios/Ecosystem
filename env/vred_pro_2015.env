    'PYTHONPATH': 
        '${RS_PY_SITE_PACKAGES};' \
        '${RS_IACTIVE_TOOLS}/python;' \
        '${RS_IACTIVE_TOOLS}/vred;' \
        '${RS_IACTIVE_TOOLS}/maya;' \
        '${RS_IACTIVE_TOOLS}/maya/scripts',
    
    'PATH': '${RS_LAUNCHER_VRED}'
    },

