            'RS_PY_SITE_PACKAGES': '${RS_LAUNCHER_VRED}/env/py2.7.8x32/lib/site-packages',
        },
    'devjc': {
            'CHANNEL': 'dev',
            
               'RS_STUDIO_DEPLOY': '${RS_DEV_AREA}/studio',
               'RS_IACTIVE_TOOLS': '${RS_STUDIO_DEPLOY}/tools/python/src/iactive',
